# MyAnonamouse Credentials
MAM_USERNAME=your_username
MAM_PASSWORD=your_password
MAM_BASE_URL=https://www.myanonamouse.net

# Authentication Mode
# Options: "cookies" (use existing Firefox profile) or "form" (scripted login)
# Default: "cookies" with MAM-Scraper profile and VPN bypass
LOGIN_MODE=cookies

# Firefox Profile Path (used when LOGIN_MODE=cookies)
# Default profile created for scraper: MAM-Scraper
# Run ./login-to-profile.sh ONCE to log into MyAnonamouse before using the scraper
# The profile directory name will be different on your system (e.g., XXXXXXXX.MAM-Scraper)
# Find it with: ls ~/.mozilla/firefox/ | grep MAM-Scraper
FIREFOX_PROFILE_PATH=/home/username/.mozilla/firefox/XXXXXXXX.MAM-Scraper

# VPN Bypass: Launch Firefox through firejail to bypass VPN
# This ensures MyAnonamouse access works even when system VPN is active
# Requires firefox-no-vpn-wrapper.sh script (included in project)
# Set to False if you don't need VPN bypass
USE_VPN_BYPASS=True

# Optional: Override safe crawl settings
# MIN_DELAY_SECONDS=3
# MAX_DELAY_SECONDS=7
# MAX_PAGES_PER_SEARCH=50
# MAX_TORRENTS_TOTAL=1000

# Browser settings
BROWSER_HEADLESS=False
BROWSER_TYPE=firefox
